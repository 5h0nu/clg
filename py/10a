import plotly.graph_objects as go
import pandas as pd
dates = pd.date_range(start='2024-01-01', end='2024-01-10')
values = [5, 7, 9, 10, 8, 6, 4, 3, 5, 7]
data = pd.DataFrame({
 'Date': dates,
 'Value': values
})
fig = go.Figure()
fig.add_trace(go.Scatter(
 x=data['Date'],
 y=data['Value'],
 mode='lines',
 name='Time Series'
))
fig.update_layout(
 title='Time Series Plot using Plotly',
 xaxis_title='Date',
 yaxis_title='Value'
)
fig.show()
